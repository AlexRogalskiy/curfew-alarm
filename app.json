{
  "expo": {
    "name": "curfew-alarm",
    "slug": "curfew-alarm",
    "version": "5.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/web_hi_res_1024.png",
    "splash": {
      "image": "./assets/splash/web_hi_res_1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "notification": {
      "icon": "./assets/icon/res/mipmap-xhdpi/icon.png"
    },
    "ios": {
      "buildNumber": "5.0.0",
      "supportsTablet": true,
      "bundleIdentifier": "com.nvbn.curfewalarm"
    },
    "android": {
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/web_hi_res_1024.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true,
      "package": "com.nvbn.curfewalarm",
      "permissions": ["NOTIFICATIONS", "USER_FACING_NOTIFICATIONS"]
    },
    "web": {
      "favicon": "./assets/icon/web_hi_res_1024.png"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps"
        }
      ]
    }
  }
}
